{
  "dashboard": {
    "id": null,
    "uid": "latency",
    "title": "Latency & Throughput",
    "tags": ["ml-monitoring"],
    "timezone": "browser",
    "schemaVersion": 39,
    "version": 1,
    "refresh": "10s",
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 1,
    "links": [],
    "panels": [
      {
        "id": 1,
        "title": "Current Average Latency",
        "type": "stat",
        "gridPos": { "h": 4, "w": 8, "x": 0, "y": 0 },
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": null },
                { "color": "orange", "value": 0.2 },
                { "color": "red", "value": 0.5 }
              ]
            },
            "unit": "s",
            "decimals": 3
          },
          "overrides": []
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "targets": [
          {
            "refId": "A",
            "expr": "rate(model_prediction_latency_seconds_sum{model_name=~\"$model_name\"}[5m]) / rate(model_prediction_latency_seconds_count{model_name=~\"$model_name\"}[5m])",
            "legendFormat": "{{model_name}}",
            "instant": true
          }
        ]
      },
      {
        "id": 2,
        "title": "Predictions In Flight",
        "type": "stat",
        "gridPos": { "h": 4, "w": 8, "x": 8, "y": 0 },
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": null },
                { "color": "orange", "value": 50 },
                { "color": "red", "value": 100 }
              ]
            },
            "unit": "none",
            "decimals": 0
          },
          "overrides": []
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "targets": [
          {
            "refId": "A",
            "expr": "model_predictions_in_flight{model_name=~\"$model_name\"}",
            "legendFormat": "{{model_name}}",
            "instant": true
          }
        ]
      },
      {
        "id": 3,
        "title": "Requests Per Second",
        "type": "stat",
        "gridPos": { "h": 4, "w": 8, "x": 16, "y": 0 },
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "red", "value": null },
                { "color": "orange", "value": 1 },
                { "color": "green", "value": 10 }
              ]
            },
            "unit": "reqps",
            "decimals": 2
          },
          "overrides": []
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "targets": [
          {
            "refId": "A",
            "expr": "rate(model_predictions_total{model_name=~\"$model_name\"}[1m])",
            "legendFormat": "{{model_name}}",
            "instant": true
          }
        ]
      },
      {
        "id": 4,
        "title": "Latency Percentiles Over Time",
        "type": "timeseries",
        "gridPos": { "h": 10, "w": 24, "x": 0, "y": 4 },
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "custom": {
              "axisBorderShow": false,
              "axisCenteredZero": false,
              "axisColorMode": "text",
              "axisLabel": "Latency (seconds)",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 5,
              "gradientMode": "none",
              "hideFrom": { "legend": false, "tooltip": false, "viz": false },
              "insertNulls": false,
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": { "type": "linear" },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": { "group": "A", "mode": "none" },
              "thresholdsStyle": { "mode": "off" }
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": null }
              ]
            },
            "unit": "s"
          },
          "overrides": [
            {
              "matcher": { "id": "byName", "options": "P99" },
              "properties": [
                { "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }
              ]
            },
            {
              "matcher": { "id": "byName", "options": "P95" },
              "properties": [
                { "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }
              ]
            },
            {
              "matcher": { "id": "byName", "options": "P90" },
              "properties": [
                { "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }
              ]
            },
            {
              "matcher": { "id": "byName", "options": "P50" },
              "properties": [
                { "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }
              ]
            }
          ]
        },
        "options": {
          "tooltip": { "mode": "multi", "sort": "desc" },
          "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "max", "lastNotNull"] }
        },
        "targets": [
          {
            "refId": "A",
            "expr": "histogram_quantile(0.50, rate(model_prediction_latency_seconds_bucket{model_name=~\"$model_name\"}[5m]))",
            "legendFormat": "P50",
            "range": true
          },
          {
            "refId": "B",
            "expr": "histogram_quantile(0.90, rate(model_prediction_latency_seconds_bucket{model_name=~\"$model_name\"}[5m]))",
            "legendFormat": "P90",
            "range": true
          },
          {
            "refId": "C",
            "expr": "histogram_quantile(0.95, rate(model_prediction_latency_seconds_bucket{model_name=~\"$model_name\"}[5m]))",
            "legendFormat": "P95",
            "range": true
          },
          {
            "refId": "D",
            "expr": "histogram_quantile(0.99, rate(model_prediction_latency_seconds_bucket{model_name=~\"$model_name\"}[5m]))",
            "legendFormat": "P99",
            "range": true
          }
        ]
      },
      {
        "id": 5,
        "title": "Requests Per Second Over Time",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 },
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "custom": {
              "axisBorderShow": false,
              "axisCenteredZero": false,
              "axisColorMode": "text",
              "axisLabel": "Requests/sec",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 20,
              "gradientMode": "scheme",
              "hideFrom": { "legend": false, "tooltip": false, "viz": false },
              "insertNulls": false,
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": { "type": "linear" },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": { "group": "A", "mode": "none" },
              "thresholdsStyle": { "mode": "off" }
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": null }
              ]
            },
            "unit": "reqps"
          },
          "overrides": []
        },
        "options": {
          "tooltip": { "mode": "multi", "sort": "desc" },
          "legend": { "displayMode": "list", "placement": "bottom", "calcs": ["mean", "max"] }
        },
        "targets": [
          {
            "refId": "A",
            "expr": "rate(model_predictions_total{model_name=~\"$model_name\"}[1m])",
            "legendFormat": "{{model_name}}",
            "range": true
          }
        ]
      },
      {
        "id": 6,
        "title": "Predictions In Flight Over Time",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 14 },
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "custom": {
              "axisBorderShow": false,
              "axisCenteredZero": false,
              "axisColorMode": "text",
              "axisLabel": "In-Flight Predictions",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 15,
              "gradientMode": "none",
              "hideFrom": { "legend": false, "tooltip": false, "viz": false },
              "insertNulls": false,
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": { "type": "linear" },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": { "group": "A", "mode": "none" },
              "thresholdsStyle": { "mode": "off" }
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": null }
              ]
            },
            "unit": "none"
          },
          "overrides": []
        },
        "options": {
          "tooltip": { "mode": "multi", "sort": "desc" },
          "legend": { "displayMode": "list", "placement": "bottom", "calcs": ["mean", "max"] }
        },
        "targets": [
          {
            "refId": "A",
            "expr": "model_predictions_in_flight{model_name=~\"$model_name\"}",
            "legendFormat": "{{model_name}}",
            "range": true
          }
        ]
      }
    ],
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": { "type": "grafana", "uid": "-- Grafana --" },
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "enable": true,
          "expr": "model_predictions_total{model_name=~\"$model_name\"} > 0",
          "iconColor": "blue",
          "name": "Model Deployments",
          "titleFormat": "Model Deployed",
          "tagKeys": "model_name",
          "type": "tags"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "enable": true,
          "expr": "data_drift_score{model_name=~\"$model_name\"} > 0",
          "iconColor": "red",
          "name": "Drift Alerts",
          "titleFormat": "Drift Detected",
          "tagKeys": "model_name",
          "type": "tags"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "name": "model_name",
          "label": "Model Name",
          "type": "query",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "query": "label_values(model_prediction_latency_seconds_count, model_name)",
          "refresh": 2,
          "sort": 1,
          "includeAll": true,
          "allValue": ".*",
          "current": { "selected": true, "text": "All", "value": "$__all" },
          "multi": false
        }
      ]
    },
    "time": { "from": "now-1h", "to": "now" },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"]
    }
  },
  "overwrite": true
}
